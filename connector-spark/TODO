Aggregation test
Schema
  key
  value
  partition

select count(*), sum(value) from table group by key
select count(*), sum(value) from table group by partition
select count(*), sum(value) from table group by partition, key
select count(*), sum(value) from table group by fn(partition) , fn(key)
select count(*), sum(value) from table group by fn(partition, key)


